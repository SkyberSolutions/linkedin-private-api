{
  "name": "linkedin-private-api",
  "version": "1.1.2",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Eilon Mor",
  "license": "MIT",
  "keywords": [
    "linkedin-nodejs-api",
    "linkedin-api",
    "linked-private-api",
    "linked-unofficial-api",
    "linkedin-web-api"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/skybersolutions/linkedin-private-api.git"
  },
  "homepage": "https://github.com/skybersolutions/linkedin-private-api",
  "files": [
    "dist"
  ],
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint -c .eslintrc.js --ext .ts src",
    "typecheck": "eslint --ext .ts --ext .js src",
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "test": "NODE_ENV=test jest",
    "test:coverage": "npm run test -- --coverage",
    "prepublish": "npm run docs && npm run build",
    "prepare": "npm run docs && npm run build",
    "docs": "rimraf docs && typedoc --plugin typedoc-plugin-markdown --out docs src"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "axios": "^0.21.0",
    "cookie": "^0.5.0",
    "fs": "^0.0.1-security",
    "lodash": "^4.17.15",
    "querystring": "^0.2.0",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.22.15",
    "@faker-js/faker": "^8.2.0",
    "@types/config": "^0.0.41",
    "@types/cookie": "^0.5.3",
    "@types/jest": "^29.5.6",
    "@types/lodash": "^4.14.200",
    "@types/node": "^18.18.7",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "faked": "^2.1.0",
    "husky": "^8.0.1",
    "intermock": "^0.2.2",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "pretty-quick": "^3.0.2",
    "rimraf": "^5.0.5",
    "testdouble": "^3.18.0",
    "testdouble-jest": "^2.0.0",
    "ts-jest": "^29.1.1",
    "typedoc": "^0.25.3",
    "typedoc-plugin-markdown": "^3.16.0",
    "typescript": "^5.2.2"
  }
}
